<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dailiusprograming.rolldice">

    <application
        android:allowBackup="true"
        android:name=".util.AppPrefInit"
        android:icon="@mipmap/ic_dice"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_dice_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Rolldice">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Rolldice.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>